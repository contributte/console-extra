{
  "name": "contributte/console-extra",
  "description": "Nette-based console commands for latte, DIC, security, utils and many others",
  "keywords": [
    "nette",
    "symfony",
    "console"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/console-extra",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/di": "^3.1.8",
    "symfony/console": "^6.4.2 || ^7.0.2"
  },
  "require-dev": {
    "contributte/console": "~0.10",
    "latte/latte": "^3.0.12",
    "nette/application": "^3.1.3",
    "nette/bootstrap": "^3.1.1",
    "nette/caching": "^3.1.1",
    "nette/security": "^3.1.1",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.5.0"
  },
  "conflict": {
    "nette/schema": "<1.0.1"
  },
  "suggest": {
    "contributte/console": "Symfony\\Console for Nette"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Console\\Extra\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
